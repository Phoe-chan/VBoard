<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
	<link type='text/css' href='/stylesheets/ui-lightness/jquery-ui-1.8.24.custom.css' rel='stylesheet' />
        <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body data-role='page'>
    <input type="hidden" id="boardId" name="boardId" value="<%= boardDetails.id %>" />
    <input type="hidden" id="mapName" name="mapName" value="<%= boardDetails.mapName %>" />
    <div class='topBar' data-role='header'>
      <div id='log' class='logWindow'>
      </div>
      <div id='trash' class='trash'>Destroy</div>
    </div>
    <div id='main' data-role='content'>
      <div id='toolBox' class='toolBox'>
        <div id='actors' class='actors'></div>
        <div id='addActor' class='addActor'>
          <input id='actorName' type='text' class='addInput'/><div id='addButton' class='addButton ui-icon ui-icon-plusthick'></div>
        </div>
      </div>
      <div id='map' class='map'>
      </div>
    </div>  
    <script type='text/javascript' src='/javascripts/jquery-1.8.2.min.js'></script>
    <script type='text/javascript' src='/javascripts/jquery-ui-1.8.24.custom.min.js'></script>
    <script type='text/javascript' src='/javascripts/jquery.watermark.min.js'></script>
    <script src='/socket.io/socket.io.js'></script>
    <script type='text/javascript' src='/javascripts/clientProcessing.js'></script>
  </body>
</html>
